fn main() {
    let x = 10; // متغیر صحیح
    let y = 5;

    if x > y {
        println!("x بزرگتر از y است!");
    } else {
        println!("y بزرگتر از x است!");
    }

    let sum = add(x, y); // فراخوانی تابع
    println!("مجموع x و y برابر با {}", sum);
}

fn add(a: i32, b: i32) -> i32 {
    return a + b; // جمع دو عدد
}
